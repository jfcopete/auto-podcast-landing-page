{
  "AWSEBDockerrunVersion": "1",
  "Image": {
    "Name": "auto-podcast-backend:latest",
    "Update": "true"
  },
  "Ports": [
    {
      "ContainerPort": 5000,
      "HostPort": 80
    }
  ],
  "Volumes": [
    {
      "HostDirectory": "/var/app/data",
      "ContainerDirectory": "/data"
    }
  ],
  "Logging": "/var/log/nginx"
} 